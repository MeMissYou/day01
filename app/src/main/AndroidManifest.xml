            android:name="com.example.surprise.Music_ZhouActivity"
            android:label="@string/title_activity_Zhou" >
        </activity>
        <activity
            android:name="com.example.surprise.Music_PyinActivity"
            android:label="@string/title_activity_Pyin" >
